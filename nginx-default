#port 3000 is login-page, 
#port 5500 is me running my javascript with liveserver
#port 8085 is me running my notes
server {
	listen 80 default_server;
	listen [::]:80 default_server;

	server_name _;

	location ^~ /api/note {
		proxy_pass http://localhost:8085;
	}

	location /home.html {
		proxy_pass http://localhost:5500;
	}

	location /functionality.js {
		proxy_pass http://localhost:5500;
	}

	location / {
		proxy_pass http://localhost:3000;
	}
}
